{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
      "simple": {
        "format": "[%(levelname)s]: %(message)s",
        "datefmt": "%Y-%m-%dT%H:%M:%S%z"
      },
      "detailed": {
        "format": "[%(levelname)s|%(filename)s|L_%(lineno)d] %(asctime)s: %(message)s",
        "datefmt": "%Y-%m-%dT%H:%M:%S%z"
      }
    },
    "handlers": {
      "stdout": {
        "class": "logging.StreamHandler",
        "level": "WARNING",
        "formatter": "simple",
        "stream": "ext://sys.stdout"
      },
      "file": {
        "class": "logging.handlers.RotatingFileHandler",
        "level": "DEBUG",
        "formatter": "detailed",
        "filename": "birdwatcher.log",
        "maxBytes": 100000,
        "backupCount": 2
      },
      "queue_handler": {
        "class": "logging.handlers.QueueHandler",
        "queue": "multiprocessing.Queue",
        "handlers": [
          "stderr",
          "file"
        ],
        "respect_handler_level": true
      }
    },
    "loggers": {
      "root": {
        "level": "DEBUG",
        "handlers": [
          "queue_handler"
        ]
      }
    }
  }